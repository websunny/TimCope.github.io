{
  "name": "blog",
  "version": "0.0.1",
  "description": "react TypeScript blog",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "clean": "rimraf build && rimraf assets-cache",
    "babel": "babel src -d build --extensions '.ts,.tsx' --copy-files --ignore *.html",
    "build": "npm run babel && webpack -p --config webpack/webpack.config.js",
    "start": "node webpack/server.js"
  },
